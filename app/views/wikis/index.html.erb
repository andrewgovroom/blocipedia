<h1>Wikis Posts</h1>

<% @wiki.each do |wiki| %>
    <div class="media">
     <div class="media-body">
       <h4 class="media-heading">
         <%= link_to wiki.title, wiki %>
       </h4>
        <small>
           <%= wiki.body %>
       </small>
     </div>
     <div class="col-md-4">
       <%= link_to "New Wiki", new_wiki_path, class: 'btn btn-success' %>
     </div>
   </div>
<% end %>